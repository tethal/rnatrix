fun main() {
    // Integer arithmetic
    print(10 + 5);
    print(10 - 5);
    print(10 * 5);
    print(10 / 5);
    print(10 % 3);

    // Float arithmetic
    print(10.5 + 5.5);
    print(10.5 - 5.5);
    print(10.5 * 2.0);
    print(10.5 / 2.0);
    print(10.5 % 3.0);

    // Mixed int/float arithmetic (int + float)
    print(10 + 5.5);
    print(10 - 5.5);
    print(10 * 2.5);
    print(10 / 4.0);
    print(10 % 3.5);

    // Mixed int/float arithmetic (float + int)
    print(10.5 + 5);
    print(10.5 - 5);
    print(10.5 * 2);
    print(10.5 / 2);
    print(10.5 % 3);

    // Negative results
    print(5 - 10);
    print(-5.5 + 2.0);
}
